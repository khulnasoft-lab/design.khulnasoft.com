{
  "name": "design.gitlab.com",
  "version": "1.0.0",
  "description": "Design guidelines and UI components for GitLab",
  "author": "GitLab",
  "scripts": {
    "start": "yarn run download-application-css && HOST=0.0.0.0 PORT=${PORT:=2018} nuxt",
    "buildsingle": "nuxt build",
    "build": "yarn run download-application-css && NODE_ENV=production nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "download-application-css": "sh utils/download-application-css.sh"
  },
  "dependencies": {
    "@gitlab-org/gitlab-svgs": "^1.32.0",
    "@gitlab-org/gitlab-ui": "https://gitlab.com/gitlab-org/gitlab-ui/-/jobs/113597163/artifacts/raw/gitlab-ui.tz-more-component-examples.tgz",
    "@nuxtjs/axios": "^5.0.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-beta",
    "cross-env": "^5.2.0",
    "front-matter": "^3.0.0",
    "frontmatter-markdown-loader": "^1.4.0",
    "markdown-it": "^8.4.2",
    "nuxt": "^2.0.0",
    "vue-loader": "^15.4.2"
  },
  "devDependencies": {
    "@gitlab/eslint-config": "^1.1.1",
    "eslint": "^5.7.0",
    "eslint-loader": "^2.1.1",
    "node-sass": "^4.9.4",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "serve-static": "^1.13.2"
  }
}
